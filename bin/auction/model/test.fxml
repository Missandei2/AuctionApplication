<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="100.0" prefWidth="120.0"  xmlns:fx="http://javafx.com/fxml" fx:controller="auction.control.TestController">
  <center>
    <Text fx:id="message" text="Waiting..." />
  </center>
  <bottom>
    <Button onAction="#handleClickMeAction" text="Click Me" BorderPane.alignment="CENTER" />
  </bottom>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</BorderPane>
