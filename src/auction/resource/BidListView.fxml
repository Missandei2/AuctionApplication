<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="auction.control.bidderListViewController">
   <top>
      <Label prefHeight="43.0" prefWidth="48.0" text="Bid List" BorderPane.alignment="CENTER" />
   </top>
   <center>
      <TableView fx:id="tableView" prefHeight="400.0" prefWidth="389.0">
        <columns>
          <TableColumn prefWidth="75.0" text="Bidder" />
          <TableColumn prefWidth="75.0" text="Amount" />
            <TableColumn prefWidth="91.0" text="Timestamp" />
        </columns>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <BorderPane.margin>
            <Insets bottom="40.0" left="20.0" right="20.0" top="2.0" />
         </BorderPane.margin>
         <effect>
            <Glow />
         </effect>
      </TableView>
   </center>
</BorderPane>
